<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <!-- Required for correct Liferay services usage generated by ServiceBuilder -->
    <listener>
        <listener-class>
            com.liferay.portal.kernel.spring.context.PortletContextLoaderListener
        </listener-class>
    </listener>

	<servlet>
		<servlet-name>JSONRPCServlet</servlet-name>
		<servlet-class>org.jabsorb.JSONRPCServlet</servlet-class>
		<!-- init-param>
			<param-name>gzip_threshold</param-name>
			<param-value>200</param-value>
		</init-param-->
	</servlet>
	<servlet-mapping>
		<servlet-name>JSONRPCServlet</servlet-name>
		<url-pattern>/JSON-RPC/*</url-pattern>
	</servlet-mapping>

    <servlet>
        <servlet-name>FormSendServlet</servlet-name>
        <servlet-class>net.portalteam.servlet.FormSendServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormSendServlet</servlet-name>
        <url-pattern>/form_send</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FormFileServlet</servlet-name>
        <servlet-class>net.portalteam.servlet.FormFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormFileServlet</servlet-name>
        <url-pattern>/form_file</url-pattern>
    </servlet-mapping>

</web-app>